@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AddGovernmentOrganisationLayout.cshtml";
}


<div id="AjaxPaginationList">
    @Html.Partial("PartialIndividuals")
</div>

<script src="~/Content/js/pagedList.js"></script>


@if (TempData["personNotExist"] != null)
{
    <script type="text/javascript">
            $(document).ready(function () {
                $("#myModal").modal('show');
            });
    </script>
    <div id="myModal" class="modal fade" style="z-index:3000">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title">Məlumat</h4>
                </div>
                <div class="modal-body">
                    <p id="successMessage">Bu istifadəçiyə uyğun məlumatlar, sistemdə mövcud deyil</p>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">bağla</button>
                </div>
            </div>
        </div>
    </div>
}

@if (TempData["indDeleted"] != null)
{
    <script type="text/javascript">
            $(document).ready(function () {
                $("#myModal4").modal('show');
            });
    </script>
    <div id="myModal4" class="modal fade">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    <h4 class="modal-title">Məlumat</h4>
                </div>
                <div class="modal-body">
                    <p id="successMessage">Fiziki şəxs sistemdən silindi</p>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal">bağla</button>
                </div>
            </div>
        </div>
    </div>
}
