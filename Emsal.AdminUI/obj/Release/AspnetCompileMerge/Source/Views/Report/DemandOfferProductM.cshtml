@model Emsal.AdminUI.Models.ReportViewModel

@{
    ViewBag.Title = "Report";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
<div id="puserMenu">
    <script>
        $(document).ready(function () {
            $.ajax({
                url: '/Report/ProductCatalog?pId=0',
                type: 'GET',
                success: function (result) {
                    $('#puserMenu').html(result);
                    $('.select2').select2();
                    //$("select[name='adminUnit']").change();
                },
                error: function () {

                }
            });
        });
    </script>
</div>


<div id="report">
    <script>
        $(document).ready(function () {
            $.ajax({
                url: '/Report/DemandOfferProduct',
                type: 'GET',
                success: function (result) {
                    $('#report').html(result);
                },
                error: function () {

                }
            });
        });
    </script>
</div>
