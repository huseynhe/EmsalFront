@model Emsal.AdminUI.Models.OfferProductionViewModel
@*@using PagedList.Mvc;*@


@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="content">
    <div class="row">
        <div class="col-md-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">Müraciətlər</h3>
                    <!-- /.box-tools -->
                </div>
                <table class="table table-hover table-striped table-bordered" style="margin-bottom:0;">
                    <tbody>
                        <tr>
                            <th class="col-md-2">
                                Məhsulun adı<br />
                                <input onkeyup="OfferProductionSearchwd(this, 'productName')" class="form-control" placeholder="axtar" />
                            </th>
                            <th class="col-md-1">
                                Dövrülük
                            </th>
                            <th class="col-md-1">
                                Miqdarı (vahidi)
                            </th>
                            <th class="col-md-1">
                                @Html.DisplayNameFor(model => model.price)
                            </th>
                            <th class="col-md-2">
                                Təklifin ünvanı - Mənşəyi<br />
<input onkeyup="OfferProductionSearchwd(this, 'fullAddress')" class="form-control" placeholder="axtar" />
                            </th>
                            <th class="col-md-2">
                                Təklif verənin soyadı, adı, atasının adı, ünvanı<br />
                                <input onkeyup="OfferProductionSearchwd(this, 'userInfo')" class="form-control" placeholder="axtar" />
                            </th>
                        </tr>
                    </tbody>
                </table>
                <div id="AjaxPaginationList">
                    @Html.Partial("PartialIndexwd")
                </div>

            </div>
        </div>
    </div>
</div>

<script src="~/Content/js/pagedList.js"></script>




