@model Emsal.UI.Models.ProductCatalogViewModel

@{
    Layout = "";
}
@if (Model.ProductCatalogList.Count() > 0)
{
    <div class="accordion-body collapse" style="display:table;">
        <div class="accordion-inner" style="margin:0;">
            <ul>
                @foreach (var item in Model.ProductCatalogList)
                {
                    <li>
                        <div><a style="cursor:pointer;" onclick="GetProductCatalog(this, @item.Id)">@item.ProductName</a>
                        <div style="margin-left:10px;" class="resp"></div>
            </div></li>
                }
                </ul>
            </div>
    </div>
}