@model Emsal.UI.Models.SpecialSummaryViewModel
@{
    ViewBag.Title = "Organisations";
}
@using PagedList.Mvc;
@{
    if (Model.OrganisationArray.Length == 0)
    {
        <div class='white_fon'>
            <p>Heç nə tapılmadı</p>
        </div>

    }
    else
    {
        foreach (var item in Model.OrganisationArray)
        {
            <div class="white_fon">
                <ul>
                    <li style="padding-left:5px; color:red" onclick="location.href='@Url.Action("Organisations", "GovernmentOrganisationSpecialSummary", new { orgId= @item.Id})'">
                        @item.name

                    </li>
                </ul>
            </div>
         }
     }
}
