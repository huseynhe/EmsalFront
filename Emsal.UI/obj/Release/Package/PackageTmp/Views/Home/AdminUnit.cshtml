@model Emsal.UI.Models.ProductCatalogViewModel

@{
    ViewBag.Title = "Region";
    Layout = "";
}

@if (Model.PRMAdminUnitList.Count > 0)
{
    <div class="form-group col-md-12" style="padding:0;">
        <select name="adId[@{ @Model.arrNum }]" onchange="GetAdminUnit(this)" class="form-control unicase-form-control select select2">
            <option selected value="">Seçim edin</option>
            @foreach (var item in Model.PRMAdminUnitList)
            {
                <option value="@item.Id">@item.Name</option>
            }
            </select>
        </div>
}
