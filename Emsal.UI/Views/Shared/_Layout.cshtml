                    <a target="_blank" href="http://docs.tedaruk.az/">
                        <div class="hex_footer">
                            <span class="fa fa-info"></span>
                        </div>
                    </a>
                    <a href="#">
                        <div class="hex_footer">
                            <span class="fa fa-facebook"></span>
                        </div>
                    </a>
                    <a href="#">
                        <div class="hex_footer">
                            <span class="fa fa-twitter"></span>
                        </div>
                    </a>
                    <a href="#">
                        <div class="hex_footer">
                            <span class="fa fa-linkedin"></span>
                        </div>
                    </a>
                    <a href="#">
                        <div class="hex_footer">
                            <span class="fa fa-rss"></span>
                        </div>
                    </a>
                </div>
            </div>
        </div>
    </div>
    <!-- footer end -->
    <script>
        $('.gallery_more').click(function () {
            var $this = $(this);
            $this.toggleClass('gallery_more');
            if ($this.hasClass('gallery_more')) {
                $this.text('Load More');
            } else {
                $this.text('Load Less');
            }
        });
    </script>


    <script src="~/Content/UI/js/jquery.validate.js"></script>
    <script src="~/Content/UI/js/jquery.validate.unobtrusive.js"></script>

    <link href="~/Content/UI/css/select2.min.css" rel="stylesheet" />
    <script src="~/Content/UI/js/select2.min.js"></script>



    <script type="text/javascript">
        $('.select2').select2();
    </script>


    <script>
        $(document).ready(function () {
            $("#puserBut").click(function () {
                $("#puserMenu").animate({
                    height: 'toggle'
                });
            });
        });
    </script>


    <script>
        $(function () {
            //$("#datepicker").datepicker();
        });
        $("#userType").on("change", function () {
            if ($("#userType").val() == "legalPerson") {
                $("#voenDiv").css("display", "block");
                $("#pinDiv").css("display", "none");
            }
            else {
                $("#voenDiv").css("display", "none");
                $("#pinDiv").css("display", "block");
            }
        })
    </script>
    <script>
        $(".numbersOnly").on("keyup", function () {
            this.value = this.value.replace(/[^0-9\.]/g, '');

            if (this.value.length > 7) {
                this.value = this.value.slice(0, 7);
            }
        })
    </script>

    <!--Start of Tawk.to Script-->
    <script type="text/javascript">
        var Tawk_API = Tawk_API || {}, Tawk_LoadStart = new Date();
        (function () {
            var s1 = document.createElement("script"), s0 = document.getElementsByTagName("script")[0];
            s1.async = true;
