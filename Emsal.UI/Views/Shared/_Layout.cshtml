    <script src="~/Content/UI/js/custom.js"></script>
    <script src="~/Content/UI/js/addPotentialUser.js"></script>
    <script src="~/Content/UI/js/emsal.js"></script>

    <script>
        function showhide() {
            var div = document.getElementById("newpost");
            if (div.style.display !== "none") {
                div.style.display = "none";
            }
            else {
                div.style.display = "block";
            }
        }
    </script>

</head>
<body>
    <div class="site-header">
        <div class="main-navigation">
            <div class="responsive_menu">
                <ul>
                    <li><a href="/"><div class="show-1 templatemo_home">Baş səhifə</div></a></li>
                    <li><a href="/Home/Announcement"><div class="show-2 templatemo_page2">Elanlar</div></a></li>
                    <li><a href="/Home/PotentialUser"><div class="show-3 templatemo_page3">Potensial istehsalçılar</div></a></li>
                    <li><a href="/OfferHome"><div class="show-4 templatemo_page4">Təkliflər</div></a></li>
                    <li><a href="/Home/Services"><div class="show-5 templatemo_page5">Keçidlər</div></a></li>
                    <li><a href="/Other/legislation"><div class="show-6 templatemo_page6">Qanunvericilik</div></a></li>
                    <li><a href="/Home/Contact"><div class="show-7 templatemo_page7">Əlaqə</div></a></li>

                    @if (Session["musername"] == null)
                    {
                    <li><a href="/Login"><div class="show-8 templatemo_page8">Daxil ol</div></a></li>
                    <li><a href="/SignUpOP"><div class="show-9 templatemo_page9">Qeydiyyat</div></a></li>
                    }
                    else
                    {
                        <li><a href="/Login/Redirect"><div class="show-8 templatemo_page8">@Session["musername"]</div></a></li>
                        <li><a href="/Login/Logout"><div class="show-9 templatemo_page9">Çıxış</div></a></li>
                    }
                </ul>
            </div>
            <div class="container">
                <div class="row templatemo_gallerygap">
                    <div class="col-md-12 responsive-menu">
                        <a href="#" class="menu-toggle-btn">
                            <i class="fa fa-bars"></i>
                        </a>
                    </div> <!-- /.col-md-12 -->

                    <div class="col-md-12 main_menu">
                        <ul>
                            <li>
                                <a href="/"><span class="fa fa-home fa-2x"></span>BAŞ SƏHİFƏ</a>
                            </li>

                            <li>
                                <a @*class="active"*@ href="/Home/Announcement"><span class="fa fa-bars"></span>ELANLAR</a>
                            </li>
                            <li>
                                <a href="/Home/PotentialUser"><span class="fa fa-users"></span>POTENSİAL İSTEHSALÇILAR</a>
                            </li>
                            <li>
                                <a href="/OfferHome"><span class="fa fa-list"></span>TƏKLİFLƏR</a>
                            </li>
                            <li>
                                <a href="/Home/Services"><span class="fa fa-cogs"></span>KEÇİDLƏR</a>
                            </li>
                            <li>
                                <a href="/Other/legislation"><span class="fa fa-book"></span>QANUNVERİCİLİK</a>
                            </li>
                            <li>
                                <a href="/Home/Contact"><span class="fa fa-envelope"></span>ƏLAQƏ</a>
                            </li>
                            @if (Session["musername"] == null)
                            {
                                <li>
                                    <a href="/Login"><span class="fa fa-lock"></span>DAXİL OL</a>
                                </li>
                                <li>
                                    <a href="/SignUpOP"><span class="fa fa-user"></span>QEYDİYYAT</a>
                                </li>
                            }
                            else
                            {
                                <li>
                                    <a href="/Login/Redirect"><span class="fa fa-user"></span>@Session["musername"]</a>
                                </li>
                                <li>
                                    <a href="/Login/Logout"><span class="fa fa-sign-out"></span>ÇIXIŞ</a>
                                </li>
                            }
                        </ul>
                    </div>
                </div>

                <div class="row search_btn">
                    <div class="col-md-3 col-sm-12">
                        <a href="/"><img src="~/Content/UI/images/logo1.png" alt="" style="height:100px;"></a>
                    </div>
                    <div class="col-md-9 col-sm-12" style="margin-top:25px;">
                        <div class="input-group">
                            <input autocomplete="off" onkeyup="searchAnnouncement(this)" type="text" class="form-control input-lg" id="formGroupInputLarge" placeholder="Elanlar üzrə axtarış ...">
                            <div style="position: absolute; width: 100%; float: left; z-index: 1000;" id="searchAnnouncementResult"></div>
                            <span class="input-group-btn">
                                <button class="btn btn-default" type="button"><span class="fa fa-search mb-0"></span></button>
                            </span>
                        </div>
                    </div>
                </div>

            </div> <!-- /.container -->
        </div> <!-- /.main-navigation -->
    </div>

    <div class="loader"></div>
    <div>
        @RenderBody()
    </div>
    <!-- gallery end -->
    <!-- footer start -->
