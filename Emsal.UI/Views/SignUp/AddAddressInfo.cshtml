@model Emsal.UI.Models.User
@{
    Layout = "~/Views/Shared/_SignUpLayout.cshtml";
}

<div class="container col-md-6 col-md-offset-3" style="border-radius: 5px; margin-top: 30px; margin-bottom:30px; box-shadow:1px 1px 2px 3px #ddd">
    <br /><br /><br />

    <h2 style="text-align:center">Ünvan Məlumatları</h2>
    <br /><br /><br />
    @using (Html.BeginForm("AddAddressInfo", "SignUp", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        @Html.HiddenFor(model => model.finvoenType)
        @Html.HiddenFor(model => model.finvoen)


        <div class="form-group" style="display:none">
            @Html.LabelFor(x => x.UserName)
            @Html.TextBoxFor(x => x.UserName, new { @class = "form-control", id = "username" })
        </div>
        <div class="form-group" style="padding:5px; display:none;">
            @Html.LabelFor(x => x.Pin)
            @Html.TextBoxFor(x => x.Pin, new { @class = "form-control", id = "pin" })
        </div>
        <div class="form-group" style="padding:5px; display:none;">
            @Html.LabelFor(x => x.Voen)
            @Html.TextBoxFor(x => x.Voen, new { @class = "form-control", id = "voen" })
        </div>
        <div class="form-group" style="display:none">
            @Html.LabelFor(x => x.Password)
            @Html.TextBoxFor(x => x.Password, new { @class = "form-control", id = "password", autocomplete = "off" })
        </div>
        <div class="form-group" style="display:none">
            @Html.LabelFor(x => x.Email)
            @Html.TextBoxFor(x => x.Email, new { @class = "form-control", id = "email", autocomplete = "off" })
        </div>
        <div class="form-group" style="display:none">
            @Html.LabelFor(x => x.FutureUserRole)
            @Html.TextBoxFor(x => x.FutureUserRole, new { @class = "form-control", id = "futureUserRole", autocomplete = "off" })
        </div>
        

            <div class="form-group" id="signUpAdminUnit">
                <script>
                        $(document).ready(function () {
                            $.ajax({
                                url: '/SignUp/AdminUnit?pId=0',
                                type: 'GET',
                                success: function (result) {
                                    $('#signUpAdminUnit').html(result);

                                    $("select[name='adId[0]']").change();
                                },
                                error: function () {

                                }
                            });
                        });
                </script>
                @*@Html.Action("AdminUnit", "OfferProduction", new { pId = 0 })*@
            </div>
            <div class="clearfix"></div><div class="clearfix"></div>
            @*<div class="form-group" style="margin-left:12px; margin-right:5px">
                <label>Küçə</label>
                <select class="form-control" id="throughfare" name="Throughfare"></select>
            </div>
            <p class="text-danger" id="throughfareAlert"></p><br />*@

            <div class="col-md-6 col-md-offset-4" style="margin-bottom:20px; padding:5px">
                <input type="submit" class="btn btn-success" value="Davam Et" id="sendInfo" />
            </div>
                    }
    <div class="clearfix" style="height: 20px;"></div>

</div>
