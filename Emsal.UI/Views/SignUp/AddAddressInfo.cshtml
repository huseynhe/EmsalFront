@model Emsal.UI.Models.User
@{
    Layout = "~/Views/Shared/_SignUpLayout.cshtml";
}

<div class="container col-md-6 col-md-offset-3" style="border-radius: 5px; margin-top: 30px; margin-bottom:30px; box-shadow:1px 1px 2px 3px #ddd">
    <br /><br /><br />

    <h2 style="text-align:center">Ünvan Məlumatları</h2>
    <br /><br /><br />
    @using (Html.BeginForm("AddAddressInfo", "SignUp", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        @Html.HiddenFor(model => model.finvoenType)
        @Html.HiddenFor(model => model.uid)
        @Html.HiddenFor(model => model.UserName)
        @Html.HiddenFor(model => model.Pin)
        @Html.HiddenFor(model => model.Voen)
        @Html.HiddenFor(model => model.Password)
        @Html.HiddenFor(model => model.Email)
        @Html.HiddenFor(model => model.FutureUserRole)
        @Html.HiddenFor(model => model.MobilePhone)
        @Html.HiddenFor(model => model.mobilePhonePrefix)

            <div class="form-group" id="signUpAdminUnit">
                <script>
                        $(document).ready(function () {
                            $.ajax({
                                url: '/SignUp/AdminUnit?pId=0&adminUnitId=@Model.AdminUnitId',
                                type: 'GET',
                                success: function (result) {
                                    $('#signUpAdminUnit').html(result);

                                    $("select[name='adId[0]']").change();
                                },
                                error: function () {

                                }
                            });
                        });
                </script>
                @*@Html.Action("AdminUnit", "OfferProduction", new { pId = 0 })*@
            </div>
            <div class="clearfix"></div><div class="clearfix"></div>

            <div class="col-md-6 col-md-offset-4" style="margin-bottom:20px; padding:5px">
                <input type="submit" class="btn btn-success" value="Davam Et" id="sendInfo" />
            </div>
                    }
    <div class="clearfix" style="height: 20px;"></div>

</div>
